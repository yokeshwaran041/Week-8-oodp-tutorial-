#include <iostream>
using namespace std;

template <typename T>
void findMax(T a, T b) {
    if (a > b)
        cout << "Max value: " << a << endl;
    else
        cout << "Max value: " << b << endl;
}

int main() {
    findMax(10, 20);
    findMax(3.14f, 2.71f);
    findMax(10.5, 20.75);
    return 0;
}
